{
	"extends": ["eslint:recommended", "next", "plugin:@next/next/recommended", "prettier"],
	"rules": {
		"no-unused-vars": [
			"error",
			{
				"vars": "local",
				"varsIgnorePattern": "[iI]gnored",
				"argsIgnorePattern": "^_",
				"args": "after-used",
				"caughtErrors": "all",
				"destructuredArrayIgnorePattern": "^_",
				"ignoreRestSiblings": true
			}
		],
		"no-empty-pattern": ["error", { "allowObjectPatternsAsParameters": true }]
	}
}
